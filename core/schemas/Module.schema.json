{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "id": "Core.Module",
  "title": "Module",
  "description": "Contains all types and functions of a module",
  "type": "object",
  "properties": {
    "types": {
      "type": "object",
      "patternProperties": {
        "*": { "$ref": "core/Type.schema.json" }
      }
    },
    "functions": {
      "type": "object",
      "patternProperties": {
        "*": { "$ref": "core/Function.schema.json" }
      }
    }
  },
  "additionalProperties": false
}
