{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "Type.schema.json",
  "title": "Type",
  "type": "object",
  "properties": {
    "name": { "$ref": "Identifier.schema.json" },
    "properties": {
      "type": "array",
      "items": { "$ref": "VariableDeclaration.schema.json" }
    }
  },
  "additionalProperties": false,
  "required": ["name", "properties"]
}
